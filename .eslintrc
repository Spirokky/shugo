{
  "extends": ["google", "plugin:prettier/recommended"],
  "env": {
    "node": true,
    "mongo": true,
    "mocha": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "require-jsdoc": 0,
    "new-cap": 0
  }
}
